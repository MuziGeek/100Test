<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="com.muzi" level="info" />

    <root level="info">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>